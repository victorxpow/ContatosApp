<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>Nome</th>
      <th>Email</th>
      <th>Telefone</th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %>
      <tr id="contact-<%= contact.id %>">
        <td><%= link_to contact.name, contact %></td>
        <td><%= contact.phone %></td>
        <td><%= contact.email %></td>
    <% end %>
  </tbody>
</table>

<%= link_to 'Registrar Novo', new_contact_path, class: "btn btn-primary btn-sm"  %>
